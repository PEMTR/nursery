type Cup {
  cup: String,
  user: String,
  notice: Boolean,
  date: Int,
  update: Int
}


type User {
  _id: String,
  type: Int,
  username: String,
  password: String,
  nick_name: String,
  phone: String,
  status: Int,
  date: Int,
  update: Int,
  cups: [Cup]
}

input Match {
  _id: String,
  type: Int,
  username: String,
  password: String,
  nick_name: String,
  phone: String,
  status: Int,
  date: Int,
  update: Int
}

type Query {
  user (id: String!): User,
  users (match: Match, skip: Int, limit: Int): [User]
}