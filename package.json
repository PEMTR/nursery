{
    "name": "nursery",
    "version": "0.0.1",
    "main": "main.js",
    "license": "MIT",
    "description": "Kindergarten project service cluster.",
    "author": "Mr.Panda <xivistudios@gmail.com> (https://github.com/quasipaa)",
    "homepage": "https://github.com/quasipaa/nursery",
    "repository": {
        "type": "git",
        "url": "https://github.com/quasipaa/nursery"
    },
    "scripts": {
        "docker-build": "docker build --tag=$1 .",
        "testing": "jest",
        "test-watch": "jest --watchAll",
        "install": "npm i --unsafe-perm=true",
        "init": "cp -r ./bin ./node_modules/@mod/"
    },
    "jest": {
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "js"
        ],
        "rootDir": "./",
        "roots": [
            "./"
        ],
        "testMatch": [
            "**/.testing/**/*.js",
            "!**/node_modules/**",
            "!**/.crate.js"
        ]
    },
    "keywords": [
        "node.js",
        "mongodb",
        "rabbitmq",
        "oss",
        "elk",
        "docker",
        "gitlab",
        "moleculer"
    ],
    "devDependencies": {
        "jest": "^24.9.0"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^7.2.0",
        "@sentry/node": "^5.5.0",
        "ali-oss": "^6.1.1",
        "amqplib": "^0.5.3",
        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.4",
        "express": "^4.17.1",
        "express-graphql": "^0.9.0",
        "express-xml-bodyparser": "^0.3.0",
        "fastest-validator": "^0.6.17",
        "fluent-ffmpeg": "^2.1.2",
        "graphql": "^14.4.2",
        "json-to-pretty-yaml": "^1.2.2",
        "lazy_mod": "^0.1.5",
        "moleculer": "^0.13.10",
        "moment": "^2.24.0",
        "mongodb": "^3.3.0-beta2",
        "nats": "^1.3.0",
        "saslprep": "^1.0.3",
        "sharp": "^0.23.0",
        "signale": "^1.4.0",
        "uuid": "^3.3.2"
    }
}
